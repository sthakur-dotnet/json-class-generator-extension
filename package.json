{
  "name": "json-class-generator",
  "displayName": "JSON Class Generator",
  "description": "View JSON tree and generate C#, Java, TypeScript classes from JSON inside VS Code",
  "version": "0.0.1",
  "publisher": "suvankarthakur",
  "engines": {
    "vscode": ">=1.60.0"
  },
  "activationEvents": [
    "onCommand:jsonClassGenerator.open"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "^22.15.30",
    "@types/vscode": "^1.60.0",
    "typescript": "^4.4.3"
  },
  "contributes": {
    "commands": [
      {
        "command": "jsonClassGenerator.open",
        "title": "JSON: Format & Generate Classes"
      }
    ]
  }
}
